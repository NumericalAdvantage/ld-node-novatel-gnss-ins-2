table GPSMeasurement 
{
    timestamp: int64;   // ns since Unix Epoch
    latitude: double;   // degrees, S<0, N>0
    longitude: double;  // degrees, W<0, E>0
    altitude: double;   // metres
    gpsQuality: uint8;  // GPS quality according to NMEA standard
}

table AccData
{
    timestamp: int64;   // ns since Unix Epoch
    accownX: int64;
    accownY: int64;
    accownZ: int64;
}

table GyrData
{
    timestamp: int64;   // ns since Unix Epoch
    gyrownX: int64;
    gyrownY: int64;
    gyrownZ: int64;
}
